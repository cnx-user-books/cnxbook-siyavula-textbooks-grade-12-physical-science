                      <m:mn>2</m:mn>
                      <m:msup>
                        <m:mi>e</m:mi>
                        <m:mo>-</m:mo>
                      </m:msup>
                      <m:mo>⇌</m:mo>
                      <m:mi>C</m:mi>
                      <m:mi>u</m:mi>
                    </m:mrow>
                  </m:math>
                </entry>
                <entry>0.34</entry>
              </row>
              <row>
                <entry>
                  <m:math overflow="scroll">
                    <m:mrow>
                      <m:mi>H</m:mi>
                      <m:msup>
                        <m:mi>g</m:mi>
                        <m:mrow>
                          <m:mn>2</m:mn>
                          <m:mo>+</m:mo>
                        </m:mrow>
                      </m:msup>
                      <m:mo>+</m:mo>
                      <m:mn>2</m:mn>
                      <m:msup>
                        <m:mi>e</m:mi>
                        <m:mo>-</m:mo>
                      </m:msup>
                      <m:mo>⇌</m:mo>
                      <m:mi>H</m:mi>
                      <m:mi>g</m:mi>
                      <m:mrow>
                        <m:mo>(</m:mo>
                        <m:mi>l</m:mi>
                        <m:mo>)</m:mo>
                      </m:mrow>
                    </m:mrow>
                  </m:math>
                </entry>
                <entry>0.78</entry>
              </row>
              <row>
                <entry>
                  <m:math overflow="scroll">
                    <m:mrow>
                      <m:mi>A</m:mi>
                      <m:msup>
                        <m:mi>g</m:mi>
                        <m:mo>+</m:mo>
                      </m:msup>
                      <m:mo>+</m:mo>
                      <m:msup>
                        <m:mi>e</m:mi>
                        <m:mo>-</m:mo>
                      </m:msup>
                      <m:mo>⇌</m:mo>
                      <m:mi>A</m:mi>
