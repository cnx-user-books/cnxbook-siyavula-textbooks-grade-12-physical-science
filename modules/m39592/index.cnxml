                        <m:mn>2</m:mn>
                      </m:mfrac>
                      <m:mi>m</m:mi>
                      <m:msubsup>
                        <m:mi>v</m:mi>
                        <m:mrow>
                          <m:mi>A</m:mi>
                        </m:mrow>
                        <m:mn>2</m:mn>
                      </m:msubsup>
                      <m:mo>+</m:mo>
                      <m:mi>m</m:mi>
                      <m:mi>g</m:mi>
                      <m:msub>
                        <m:mi>h</m:mi>
                        <m:mi>A</m:mi>
                      </m:msub>
                    </m:mrow>
