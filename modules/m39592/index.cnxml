                <m:mtr>
                  <m:mtd/>
                  <m:mtd>
                    <m:mo>=</m:mo>
                  </m:mtd>
                  <m:mtd columnalign="left">
                    <m:mrow>
                      <m:mfrac>
                        <m:mn>1</m:mn>
                        <m:mn>2</m:mn>
                      </m:mfrac>
                      <m:mi>m</m:mi>
                      <m:msubsup>
                        <m:mi>v</m:mi>
                        <m:mrow>
                          <m:mi>A</m:mi>
