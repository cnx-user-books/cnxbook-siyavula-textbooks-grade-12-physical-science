                  <m:mo>=</m:mo>
                </m:mtd>
                <m:mtd columnalign="left">
                  <m:mrow>
                    <m:msub>
                      <m:mi>F</m:mi>
                      <m:mi>g</m:mi>
                    </m:msub>
                    <m:mo>·</m:mo>
                    <m:msub>
