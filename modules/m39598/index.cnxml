            <m:mi>m</m:mi>
            <m:mover accent="true">
              <m:mi>v</m:mi>
              <m:mo>→</m:mo>
            </m:mover>
          </m:mrow>
        </m:math>
      </equation>
      <para id="id193048"><emphasis effect="bold">Kinetic energy</emphasis>:</para>
      <equation id="uid78">
        <m:math overflow="scroll" mode="display">
          <m:mrow>
            <m:msub>
