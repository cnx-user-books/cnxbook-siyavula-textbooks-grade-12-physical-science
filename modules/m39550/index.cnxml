                <m:mtd columnalign="left">
                  <m:mrow>
                    <m:msub>
                      <m:mi>m</m:mi>
                      <m:mn>2</m:mn>
                    </m:msub>
                    <m:msub>
                      <m:mi>v</m:mi>
                      <m:mrow>
