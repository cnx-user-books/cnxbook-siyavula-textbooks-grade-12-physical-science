                    </m:msub>
                    <m:mo>=</m:mo>
                    <m:msub>
                      <m:mi>m</m:mi>
                      <m:mn>1</m:mn>
                    </m:msub>
                    <m:msub>
                      <m:mi>v</m:mi>
                      <m:mrow>
                        <m:mi>i</m:mi>
                        <m:mn>1</m:mn>
                      </m:mrow>
                    </m:msub>
                    <m:mo form="prefix">cos</m:mo>
                    <m:msub>
                      <m:mi>θ</m:mi>
                      <m:mn>1</m:mn>
                    </m:msub>
                  </m:mrow>
                </m:mtd>
              </m:mtr>
              <m:mtr>
                <m:mtd columnalign="right">
                  <m:msub>
                    <m:mi>p</m:mi>
                    <m:mrow>
                      <m:mi>i</m:mi>
                      <m:mn>2</m:mn>
                      <m:mi>x</m:mi>
                    </m:mrow>
                  </m:msub>
