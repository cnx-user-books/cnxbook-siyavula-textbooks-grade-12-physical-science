                <m:mtd columnalign="right">
                  <m:msub>
                    <m:mi>p</m:mi>
                    <m:mrow>
                      <m:mi>i</m:mi>
                      <m:mn>1</m:mn>
                      <m:mi>x</m:mi>
                    </m:mrow>
                  </m:msub>
                </m:mtd>
                <m:mtd>
                  <m:mo>=</m:mo>
                </m:mtd>
                <m:mtd columnalign="left">
                  <m:mrow>
                    <m:msub>
                      <m:mi>m</m:mi>
                      <m:mn>1</m:mn>
                    </m:msub>
                    <m:msub>
                      <m:mi>v</m:mi>
                      <m:mrow>
                        <m:mi>i</m:mi>
                        <m:mn>1</m:mn>
                        <m:mi>x</m:mi>
