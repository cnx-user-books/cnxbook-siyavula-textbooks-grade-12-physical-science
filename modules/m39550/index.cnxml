                    <m:msub>
                      <m:mi>p</m:mi>
                      <m:mrow>
                        <m:mi>i</m:mi>
                        <m:mi>x</m:mi>
                        <m:mn>1</m:mn>
                      </m:mrow>
                    </m:msub>
                    <m:mo>+</m:mo>
                    <m:msub>
                      <m:mi>p</m:mi>
                      <m:mrow>
                        <m:mi>i</m:mi>
                        <m:mi>x</m:mi>
                        <m:mn>2</m:mn>
