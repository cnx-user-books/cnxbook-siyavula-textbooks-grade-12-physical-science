                    </m:msub>
                  </m:mrow>
                </m:mtd>
                <m:mtd>
                  <m:mo>=</m:mo>
                </m:mtd>
                <m:mtd columnalign="left">
                  <m:mrow>
                    <m:msub>
                      <m:mi>p</m:mi>
                      <m:mrow>
                        <m:mi>f</m:mi>
                        <m:mi>x</m:mi>
                        <m:mn>1</m:mn>
                      </m:mrow>
                    </m:msub>
                    <m:mo>+</m:mo>
                    <m:msub>
                      <m:mi>p</m:mi>
                      <m:mrow>
                        <m:mi>f</m:mi>
                        <m:mi>x</m:mi>
                        <m:mn>2</m:mn>
                      </m:mrow>
                    </m:msub>
                  </m:mrow>
                </m:mtd>
              </m:mtr>
              <m:mtr>
                <m:mtd columnalign="right">
                  <m:mrow>
                    <m:mo>-</m:mo>
                    <m:mn>80</m:mn>
                    <m:mo>×</m:mo>
                    <m:mn>5</m:mn>
                    <m:mo form="prefix">cos</m:mo>
                    <m:msup>
                      <m:mn>75</m:mn>
                      <m:mo>∘</m:mo>
                    </m:msup>
                    <m:mo>+</m:mo>
                    <m:mn>80</m:mn>
                    <m:mo>×</m:mo>
                    <m:mn>6</m:mn>
                    <m:mo>×</m:mo>
                    <m:mo form="prefix">cos</m:mo>
                    <m:msup>
                      <m:mn>60</m:mn>
                      <m:mo>∘</m:mo>
                    </m:msup>
                  </m:mrow>
                </m:mtd>
                <m:mtd>
                  <m:mo>=</m:mo>
                </m:mtd>
                <m:mtd columnalign="left">
                  <m:mrow>
                    <m:mn>80</m:mn>
                    <m:mo>×</m:mo>
                    <m:msub>
                      <m:mi>v</m:mi>
                      <m:mrow>
                        <m:mi>f</m:mi>
                        <m:mi>x</m:mi>
                        <m:mn>1</m:mn>
                      </m:mrow>
                    </m:msub>
                    <m:mo>+</m:mo>
                    <m:mn>80</m:mn>
                    <m:mo>×</m:mo>
                    <m:mrow>
                      <m:mo>(</m:mo>
                      <m:mo>-</m:mo>
                      <m:mn>0</m:mn>
                      <m:mo>.</m:mo>
                      <m:mn>3</m:mn>
                      <m:mo>)</m:mo>
                    </m:mrow>
                  </m:mrow>
                </m:mtd>
              </m:mtr>
              <m:mtr>
                <m:mtd columnalign="right">
                  <m:msub>
                    <m:mi>v</m:mi>
                    <m:mrow>
                      <m:mi>f</m:mi>
                      <m:mi>x</m:mi>
                      <m:mn>1</m:mn>
                    </m:mrow>
                  </m:msub>
                </m:mtd>
                <m:mtd>
                  <m:mo>=</m:mo>
                </m:mtd>
                <m:mtd columnalign="left">
                  <m:mfrac>
                    <m:mrow>
                      <m:mo>-</m:mo>
                      <m:mn>80</m:mn>
                      <m:mo>×</m:mo>
                      <m:mn>5</m:mn>
                      <m:mo form="prefix">cos</m:mo>
                      <m:msup>
                        <m:mn>75</m:mn>
                        <m:mo>∘</m:mo>
                      </m:msup>
                      <m:mo>+</m:mo>
                      <m:mn>80</m:mn>
                      <m:mo>×</m:mo>
                      <m:mn>6</m:mn>
                      <m:mo>×</m:mo>
                      <m:mo form="prefix">cos</m:mo>
                      <m:msup>
                        <m:mn>60</m:mn>
                        <m:mo>∘</m:mo>
                      </m:msup>
                      <m:mo>-</m:mo>
                      <m:mn>80</m:mn>
                      <m:mo>×</m:mo>
                      <m:mrow>
                        <m:mo>(</m:mo>
                        <m:mo>-</m:mo>
                        <m:mn>0</m:mn>
                        <m:mo>.</m:mo>
                        <m:mn>3</m:mn>
                        <m:mo>)</m:mo>
                      </m:mrow>
                    </m:mrow>
                    <m:mn>80</m:mn>
                  </m:mfrac>
                </m:mtd>
              </m:mtr>
              <m:mtr>
                <m:mtd columnalign="right">
                  <m:msub>
                    <m:mi>v</m:mi>
                    <m:mrow>
                      <m:mi>f</m:mi>
                      <m:mi>x</m:mi>
                      <m:mn>1</m:mn>
                    </m:mrow>
                  </m:msub>
                </m:mtd>
                <m:mtd>
                  <m:mo>=</m:mo>
                </m:mtd>
                <m:mtd columnalign="left">
                  <m:mrow>
                    <m:mn>2</m:mn>
                    <m:mo>.</m:mo>
                    <m:mn>0</m:mn>
                    <m:mspace width="3.33333pt"/>
                    <m:msup>
                      <m:mi> ms </m:mi>
