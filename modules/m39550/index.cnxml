                    <m:msub>
                      <m:mi>θ</m:mi>
                      <m:mn>2</m:mn>
                    </m:msub>
                  </m:mrow>
                </m:mtd>
              </m:mtr>
            </m:mtable>
          </m:math>
        </equation>
        <para id="id240423"><m:math overflow="scroll"><m:mi>y</m:mi></m:math>-component of momentum:</para>
        <equation id="id240434">
          <m:math overflow="scroll" mode="display">
