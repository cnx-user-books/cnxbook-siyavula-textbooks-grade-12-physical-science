                    <m:mo>=</m:mo>
                  </m:mtd>
                  <m:mtd columnalign="left">
                    <m:msup>
                      <m:mrow>
                        <m:mo>(</m:mo>
                        <m:mn>4</m:mn>
                        <m:mo>,</m:mo>
                        <m:mn>9</m:mn>
                        <m:mo>)</m:mo>
                      </m:mrow>
                      <m:mn>2</m:mn>
                    </m:msup>
