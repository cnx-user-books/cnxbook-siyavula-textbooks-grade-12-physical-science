                    <m:mn>361</m:mn>
                    <m:mo>.</m:mo>
                    <m:mn>1</m:mn>
                    <m:mspace width="4pt"/>
                    <m:msup>
                      <m:mrow>
                        <m:mi mathvariant="normal">m</m:mi>
                        <m:mo>·</m:mo>
                        <m:mi mathvariant="normal">s</m:mi>
                      </m:mrow>
                      <m:mrow>
                        <m:mo>-</m:mo>
